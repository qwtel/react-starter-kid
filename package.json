{
  "name": "react-starter-kid",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "copy": "mkdir -p ./build/public; cp -r ./src/public/* ./build/public",
    "build": "npm run copy; webpack",
    "build:prod": "npm run copy; NODE_ENV=production webpack",
    "dev": "npm run copy; webpack-dev-server --progress --content-base ./build/public --history-api-fallback",
    "clean": "rm -rf ./build"
  },
  "author": "Florian Klampfer <f.klampfer@gmail.com> (http://qwtel.com)",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.8.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
