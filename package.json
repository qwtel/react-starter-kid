{
  "name": "react-starter-kid",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "copy": "mkdir -p ./build/public; cp -r ./src/public/* ./build/public/",
    "build": "npm run copy; webpack --config ./webpack.config.js",
    "build:prod": "npm run copy; NODE_ENV=production webpack --config ./webpack.config.js",
    "dev": "npm run copy; webpack --config ./webpack.config.js --watch",
    "clean": "rm -rf ./build"
  },
  "author": "Florian Klampfer <f.klampfer@gmail.com> (http://qwtel.com)",
  "license": "MIT",
  "dependencies": {
    "react": "^15.0.2",
    "react-dom": "^15.0.2"
  },
  "devDependencies": {
    "webpack": "^1.13.0"
  }
}
